# GIMP user context

(tool "gimp-paintbrush-tool")
(paint-info "gimp-paintbrush")
(foreground (color-rgb 0.980392 0.023529 0.956863))
(background (color-rgb 0.000000 0.000000 0.000000))
(paint-mode darken-only-mode)
(brush "2. Hardness 050")
(dynamics "Pressure Opacity")
(pattern "Stone")
(gradient "FG to BG (HSV clockwise hue)")
(palette "Bears")
(font "Ubuntu")

# end of user context
