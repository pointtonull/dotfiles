#!/usr/bin/env sh
set -e

pwd
gt.commit


. ./.env
make echo|awk '
    /VERSION/{print "\033[1m" $1, $2, "\033[32m" $3 "\033[0m"; next}
    /S3_BUCKET/{print "\033[1m" $1, $2, "\033[32m" $3 "\033[0m"; next}
    1
'
make upload

. ./.prod
make echo|awk '
    /VERSION/{print "\033[1m" $1, $2, "\033[31m" $3 "\033[0m"; next}
    /S3_BUCKET/{print "\033[1m" $1, $2, "\033[31m" $3 "\033[0m"; next}
    1
'
make upload
