# Order matters
# more specific matches should be on top
application/pdf;PDF Document;evince %f
application/epub;EPUB Document;fbreader %f
image/vnd.djvu;Djvu Document;djview %f
video/;Video;smplayer -fullscreen %f
image/;Image;gpicview %f
inode/directory;Directory;vimrenamer -ls %f
inode/symlink;Link;vimrenamer -ls %f
application/vnd.ms-excel;Excel;libreoffice %f
text/html;HTML Document;firefox %f
text/plain;Text Document;gvim -f %f
application/vnd.oasis.opendocument;OpenDocument;libreoffice %f
